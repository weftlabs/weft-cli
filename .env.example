# AI Workflow Configuration Template
# Copy this file to .env and fill in your values

# Path to the code repository being developed
CODE_REPO_PATH=

# Path to the AI history repository (stores all AI interactions)
AI_HISTORY_PATH=

# Claude API key for AI backend (obtain from https://console.anthropic.com/)
CLAUDE_API_KEY=

# Claude model to use for AI agents
CLAUDE_MODEL=claude-3-5-sonnet-20241022

# Polling interval for watchers (in seconds)
POLL_INTERVAL=2
